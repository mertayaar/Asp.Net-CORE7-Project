@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("/Views/Admin/PartialHead.cshtml")
<body>
    <div class="container-scroller">
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
            <div class="sidebar-brand-wrapper d-none d-lg-flex align-items-center justify-content-center fixed-top">
                <a class="sidebar-brand brand-logo" href="~/Corona-template-1.0.0/template/index.html"><img src="~/Corona-template-1.0.0/template/assets/images/logo.svg" alt="logo" /></a>
                <a class="sidebar-brand brand-logo-mini" href="~/Corona-template-1.0.0/template/index.html"><img src="~/Corona-template-1.0.0/template/assets/images/logo-mini.svg" alt="logo" /></a>
            </div>
            @await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
        </nav>
        <div class="container-fluid page-body-wrapper">
            @await Html.PartialAsync("/Views/Admin/PartialNavbar.cshtml")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()

                </div>
                @await RenderSectionAsync("scripts", required: false)
                <!-- content-wrapper ends -->
                <!-- partial:../../partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright Â© bootstrapdash.com 2020</span>
                        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center"> Free <a href="https://www.bootstrapdash.com/bootstrap-admin-template/" target="_blank">Bootstrap admin templates</a> from Bootstrapdash.com</span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>

    @await Html.PartialAsync("/Views/Admin/PartialScript.cshtml")

</body>
</html>

